     ]
    }
   ],
   "source": [
    "%%time\n",
    "coil_feature_vectors = []\n",
    "for filename in filelist:\n",
    "    im = imageio.imread(data_folder + '/' + filename)\n",
    "    coil_feature_vectors.append(im.flatten())\n",
    "coil_20_data = np.asarray(coil_feature_vectors)\n",
    "coil_20_target = pd.Series(filelist).str.extract(\"obj([0-9]+)\", expand=False).values.astype(np.int32)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 25,
   "id": "1e5137d9",
   "metadata": {},
   "outputs": [],
   "source": [
    "raw_coil = coil_20_data.astype(np.float32)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 26,
   "id": "0db259a3",
   "metadata": {},
   "outputs": [],
   "source": [
    "A_coil, sigmas, rhos, dists = umap.umap_.fuzzy_simplicial_set(X=raw_coil,\n",
    "                                                            n_neighbors=5,  \n",
    "                                                            random_state=0, \n",
    "                                                         metric='euclidean', \n",
    "                                                         return_dists=True)\n",
    "G = nx.from_scipy_sparse_matrix(A_coil, edge_attribute='umap_weight')\n",
    "\n",
    "umap_coil = umap.UMAP(n_neighbors=5, n_components=4, min_dist=1e-8, random_state=0, n_epochs=1000).fit_transform(raw_coil)\n",
    "hd_umap_labels = hdbscan.HDBSCAN(min_samples=3, min_cluster_size=20).fit_predict(umap_coil)"
   ]
  },
