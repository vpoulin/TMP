    "# MNIST Clustering"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 31,
   "id": "5d01d2a4",
   "metadata": {},
   "outputs": [],
   "source": [
    "raw_mnist = mnist.data.astype(np.float32)[:35000].to_numpy()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 32,
   "id": "2ef829c6-d16f-4d97-857b-ca0bbbda9676",
   "metadata": {},
   "outputs": [],
   "source": [
    "A_mnist, sigmas, rhos, dists = umap.umap_.fuzzy_simplicial_set(X=raw_mnist,\n",
    "                                                            n_neighbors=10,  \n",
    "                                                            random_state=42, \n",
    "                                                         metric='euclidean', \n",
    "                                                         return_dists=True)\n",
    "G = nx.from_scipy_sparse_matrix(A_mnist, edge_attribute='umap_weight')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "6819fe0e",
   "metadata": {},
   "outputs": [],
   "source": [
    "umap_mnist = umap.UMAP(n_neighbors=10, n_components=4, min_dist=1e-8, random_state=42, n_epochs=500).fit_transform(raw_mnist)\n",
    "hd_umap_labels = hdbscan.HDBSCAN(min_samples=10, min_cluster_size=100).fit_predict(umap_mnist)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "130a63dd-fa5e-47e1-91bf-1b6caebb407a",
   "metadata": {},
   "outputs": [],
   "source": [
