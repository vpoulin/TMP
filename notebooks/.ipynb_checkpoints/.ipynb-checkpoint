   "metadata": {},
   "outputs": [],
   "source": [
    "vertex_labels = { 'true_label' : mnist.target[:35000],\n",
    "                  'hd_label' : hd_umap_labels,\n",
    "                }\n",
    "\n",
    "G_mnist = enrich_graph_edge_properties(G, \n",
    "                                           vertex_high_representation=raw_mnist, \n",
    "                                           vertex_low_representation=umap_mnist, \n",
    "                                           vertex_labels = vertex_labels)\n",
    "\n",
    "edge_df = nx.to_pandas_edgelist(G_mnist)\n",
    "edge_df['true_vs_hd'] = list(zip(edge_df.true_label, edge_df.hd_label))\n",
    "edge_df"
   ]
  },
