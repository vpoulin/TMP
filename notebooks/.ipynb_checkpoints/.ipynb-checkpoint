   "outputs": [],
   "source": [
    "import re\n",
    "images_zip = zipfile.ZipFile(f'{data_folder}/coil20.zip')\n",
    "mylist = images_zip.namelist()\n",
    "r = re.compile(\".*\\.png$\")\n",
    "filelist = list(filter(r.match, mylist))\n",
    "images_zip.extractall(data_folder + '/.')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 24,
   "id": "00a3e014-7df3-488e-91f9-5565dea6f82f",
   "metadata": {},
   "outputs": [
