    "                  'hd_label' : hd_umap_labels,\n",
    "                }\n",
    "\n",
    "G_usps = enrich_graph_edge_properties(G, \n",
    "                                           vertex_high_representation=raw_usps, \n",
    "                                           vertex_low_representation=umap_usps, \n",
    "                                           vertex_labels = vertex_labels)\n",
    "\n",
    "edge_df = nx.to_pandas_edgelist(G_usps)\n",
    "edge_df['true_vs_hd'] = list(zip(edge_df.true_label, edge_df.hd_label))\n",
    "edge_df"
   ]
  },
